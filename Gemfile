source "https://rubygems.org"

# This Gemfile is set up for GitHub Pages
# To update dependencies: run `bundle update github-pages`
gem "github-pages", group: :jekyll_plugins

# You can still use the Minima theme if needed
gem "minima"

# Plugins can go here
group :jekyll_plugins do
  gem "jekyll-feed"
end

# Windows fix
gem 'tzinfo-data', platforms: [:mingw, :mswin, :x64_mingw, :jruby]
gem "jekyll-paginate", "~> 1.1"
